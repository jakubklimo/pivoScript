dejmi pivo = 5;
dejmi vodka = 0b010110;
dejmi rum = 0x2a4F;
dejmi jedno = 2;
dejmi druhy = 1;

kecni(vodka);
kecni(rum);

dejmi vysledek = jedno + druhy;
kecni(vysledek);

hele (vysledek == 3) {
    kecni("To je jasný!");
} jinac {
    kecni("To není ono.");
}

jestejedno (dejmi i = 0; i < pivo; i = i + 1) {
    kecni("Ještě jedno pivo!");
}

hele (pivo > rum nebojak rum > jedno) {
    kecni("Pivo je lepší než rum.");
}

hele (nenekamo(jedno < druhy)) {
    kecni("Jednička není větší než dvojka.");
}
